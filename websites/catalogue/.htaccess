RewriteCond %{HTTP_HOST} ^animev2.fansubs.cat$ [OR]
RewriteCond %{HTTP_HOST} ^manga.fansubs.cat$ [OR]
RewriteCond %{HTTP_HOST} ^mangav2.fansubs.cat$
